<h3>User Stories</h3>

<a href="/userStories/create/<%= projectId %> " class="mdl-button mdl-js-button mdl-button--raised">
    Create user story
</a>

<ul class="mdl-list">
    <% userStories.forEach(userStory=> {%>
        <li class="mdl-list__item">

            <span class="mdl-list__item-primary-content">
                <i class="material-icons mdl-list__item-icon">dashboard</i>
                <a href="/userStories/<%= userStory._id %>">
                    <%= userStory.title %>
                </a>
            </span>

            <a href="/projects/update/<%= userStory._id %>" class="mdl-button mdl-js-button mdl-button--fab">
                <i class="material-icons">edit</i>
            </a>
            <a href="/projects/delete/<%= userStory._id %>"
                class="mdl-button mdl-js-button mdl-button--fab mdl-button--colored">
                <i class="material-icons">delete</i>
            </a>
        </li>
        <%}); %>
</ul>