<h3>bugs</h3>

<a href="/bugs/create/<%= projectId %> " class="mdl-button mdl-js-button mdl-button--raised">
    Create a report of a bug
</a>

<ul class="mdl-list">
    <% bugs.forEach(bugs=> {%>
        <li class="mdl-list__item">

            <span class="mdl-list__item-primary-content">
                <i class="material-icons mdl-list__item-icon">dashboard</i>
                <a href="/userStories/<%= bugs._id %>">
                    <%= bugs.title %>
                </a>
            </span>

            <a href="/bugs/update/<%= bugs._id %>" class="mdl-button mdl-js-button mdl-button--fab">
                <i class="material-icons">edit</i>
            </a>
            <a href="/bugs/delete/<%= bugs._id %>"
               class="mdl-button mdl-js-button mdl-button--fab mdl-button--colored">
                <i class="material-icons">delete</i>
            </a>
        </li>
    <%}); %>
</ul>